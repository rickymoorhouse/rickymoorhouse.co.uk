---
---
{
  "type": "FeatureCollection",
  "features": [{% for place in site.travel %}
    {
      "type": "Feature",
      "properties": {
        "title": "{{ place.title }}",
        "image": "{{ place.image }}",
        "url": "{{place.url}}",
        "year": "{{ place.date | date: "%Y" }}",
        "marker-size": "small",
        "marker-color": "#9900cc"
      },
      "geometry": { 
        "type": "Point",
        "coordinates": [{{ place.location.lng }},{{ place.location.lat }}]
      }
    }{% if forloop.rindex > 1 %},{% endif%}{% endfor %}
  ]
}
